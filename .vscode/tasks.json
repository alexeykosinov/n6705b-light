{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "MSYS2-GCC C++ BUILD MODE",
            "command": "c:/msys64/mingw64/bin/g++.exe",
            "args": [
                "-Wall",
                "-static",
                "-static-libgcc",
                "-static-libstdc++",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
                "-I",
                "${fileDirname}\\include",
                "-L",
                "${fileDirname}\\lib",
                "-lvisa64",
                "${fileDirname}\\${fileBasenameNoExtension}.res"
            ],
            "options": {
                "cwd": "c:/msys64/mingw64/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "compiler: c:/msys64/mingw64/bin/g++.exe"
        }
    ],
    "version": "2.0.0"
}